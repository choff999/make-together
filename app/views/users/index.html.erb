<main class="Site-content">
  <div class="container">

    <h1>Users index page</h1>

    <h2>Developers</h2>
    <% @developers.each do |developer| %>
      <div class="box">
        <p><%= link_to developer.visible_name, user_path(developer) %></p>
      </div>
    <% end %>

    <h2>Designers</h2>
    <% @designers.each do |designer| %>
      <div class="box">
        <%= link_to designer.visible_name, user_path(designer) %>
      </div>
    <% end %>

  </div>
</main>
