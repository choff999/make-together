<%= form_for @project do |f| %>
 <%= f.label :title %>
 <%= f.text_field :title, autofocus: true %>

 <%= f.label :tagline %>
 <%= f.text_field :tagline %>

 <%= f.label :description %>
 <%=f.text_field :description %>

 <%= f.label :expiration %>
 <%= f.date_field :expiration %>


 <%= f.hidden_field :user_id, value: current_user.id %>
 <%= f.submit %>
<%end%>